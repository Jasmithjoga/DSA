class Solution {
public:
    void merge(vector<int>& nums1, int m, vector<int>& nums2, int n) {
        nums1.resize(m);
        nums1.insert(nums1.end(), nums2.begin(), nums2.end());
        int size=nums1.size();
        for(int i=0; i<size-1; i++){
            for(int j=0; j<size-i-1; j++){
                if(nums1[j]>nums1[j+1]){
                    swap(nums1[j+1], nums1[j]);
                }
            }
        }
    }
};
