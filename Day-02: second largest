class Solution {
  public:
    int getSecondLargest(vector<int> &arr) {
        // code here
        int n=arr.size();
        int largest=INT_MIN, second=INT_MIN;
        for(int i=0; i<n; i++){
            if(arr[i]>largest){
                second=largest;
                largest=arr[i];
            }
            else if(arr[i]>second && arr[i]!=largest){
                second=arr[i];
            }
        }
        if(second==INT_MIN){
            return -1;
        }
        return second;
    }
};
