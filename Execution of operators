#include <bits/stdc++.h>
using namespace std;


int add(int num1, int num2){
  return num1+num2;
}
int sub(int num1, int num2){
  return num1-num2;
}
int divs(int num1, int num2){
  return num1/num2;
}
int mul(int num1, int num2){
  return num1*num2;
}
int ls(int num1, int num2){
  return num1<<num2;
}
int rs(int num1, int num2){
  return num1>>num2;
}


int main() 
{
    int x,y;
    cin>>x>>y;
    cout<<"add = "<<add(x,y)<<endl;
    cout<<"sub = "<<sub(x,y)<<endl;
    cout<<"div = "<<divs(x,y)<<endl;
    cout<<"mul = "<<mul(x,y)<<endl;
    cout<<"ls = "<<ls(x,y)<<endl;
    cout<<"rs = "<<rs(x,y)<<endl;
}



