class Solution {
  public:
    void reverseArray(vector<int> &arr) {
        // code here
        int n=arr.size();
        vector<int> temp;
        for(int i=n-1; i>=0; i--){
            temp.push_back(arr[i]);
        }
        for(int i=0; i<n; i++){
            arr[i]=temp[i];
        }
    }
};
