class Solution {
public:
    int findMaxConsecutiveOnes(vector<int>& nums) {
        int cc=0, mc=0;
        int size=nums.size();
        for(int i=0; i<size; i++){
            if(nums[i]==1){
                cc++;
               mc=  max(mc, cc);
            }
            else{
                cc=0;
            }
        }
        return mc;

    }
};
