class Solution {
  public:
    void bubbleSort(vector<int>& arr) {
        // code here
        int size=arr.size();
        for(int i=0; i<size-1; i++){
            
            for(int j=0; j<size-1-i; j++){
                
                if(arr[j]>arr[j+1]){
                    swap(arr[j+1], arr[j]);
                }
            }
        }
    }
};
